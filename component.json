{
  "title": "REST API",
  "description": "elastic.io component for the REST API",
  "docsUrl": "https://github.com/elasticio/rest-api-component",
  "credentials": {
    "fields": {
      "auth": {
        "required": false,
        "viewClass": "HTTPAuthView"
      }
    }
  },
  "triggers": {
    "httpRequestTrigger": {
      "main": "./lib/triggers/httpRequestTrigger.js",
      "title": "HTTP request",
      "type": "polling",
      "metadata": {
        "out": {}
      },
      "fields": {
          "reader": {
            "label": "REST API",
            "required": true,
            "viewClass": "RESTAPIView"
        }
      }
    }
  },
  "actions": {
    "httpRequestAction": {
      "main": "./lib/actions/httpRequestAction.js",
      "title": "HTTP request",
      "metadata": {
        "out": {}
      },
      "fields": {
        "reader": {
          "label": "REST API",
          "required": true,
          "viewClass": "RESTAPIView"
        }
      }
    }
  }
}
